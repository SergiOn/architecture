{
  "rulesDirectory": [],
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "ordered-imports": false,
    "no-console": false
  }
}
