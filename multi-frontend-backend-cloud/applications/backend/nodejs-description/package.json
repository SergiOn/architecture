{
  "name": "nodejs-description",
  "version": "1.0.0",
  "description": "NodeJS web server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "serve": "NODE_PATH=./dist node dist/server.js",
    "serve:ts": "NODE_PATH=./src ts-node ./src/server.ts",
    "watch:ts": "NODE_PATH=./src ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "SergiOn",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "@types/request": "^2.48.2",
    "@types/winston": "^2.4.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
